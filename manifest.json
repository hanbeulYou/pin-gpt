{
  "manifest_version": 3,
  "name": "ChatGPT 대화목록 고정",
  "version": "1.0",
  "description": "ChatGPT 사이드바의 대화 목록을 고정합니다",
  "permissions": ["storage"],
  "host_permissions": ["https://chat.openai.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
